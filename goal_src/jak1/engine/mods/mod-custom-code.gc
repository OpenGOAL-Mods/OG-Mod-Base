   (until (time-elapsed? current-timea (seconds 1))
   (suspend)
   )

)

((and *custom-cutscene?* (string= *currentSpoolName* "sage-intro-sequence-a") (= *currentCutsceneFrame* 2447))
   (play-rand-sound "snd/dax/oldgreenstufftoldusnotto.mp3" 100)
   (until (time-elapsed? current-timea (seconds 1))
   (suspend)
   )

