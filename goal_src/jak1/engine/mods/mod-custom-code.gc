   (play-rand-sound "snd/windy.wav" (the int (-> *setting-control* default sfx-volume)))
   (set! chem-bool #t)
   (until (or (not (is-in-chem-fac-silo)) (time-elapsed? current-timea (seconds 20)))
   (suspend)
   )
